journal = {Nucl. Instruments Methods Phys. Res. Sect. A Accel. Spectrometers, Detect. Assoc. Equip.},
keywords = {Direct mass measurement,Exotic nuclei,Isobar separator,Multiple reflection,Time-of-flight mass spectrometer,multiple re fl ection,spectrometer,time-of- fl ight mass},
pages = {172--188},
