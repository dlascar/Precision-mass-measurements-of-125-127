@article{Andersson2005,
abstract = {In the fusion-evaporation reaction Ca40+Mg24 at 104MeV beam energy, excited states have been observed for the first time in the isotope Ga316130. The experimental setup comprised the Ge array CLARION, a recoil mass spectrometer and, in its focal plane, an ionization chamber. Five transitions in Ga61 are identified, out of which a cascade of three transitions has been established by means of recoil-$\gamma$$\gamma$ coincidences. The strong transitions at 271keV in Ga61 and 124keV in Zn61 are viewed as the “mirror” 5/2−→3/2− ground-state transitions. The rather large energy difference of 150keV is suggested to arise from Coulomb monopole contributions. Shell-model calculations support this interpretation.},
