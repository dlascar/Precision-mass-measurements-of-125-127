file = {:C$\backslash$:/Daniel Lascar/Papers/Huang{\_}2017{\_}Chinese{\_}Phys.{\_}C{\_}41{\_}030002.pdf:pdf},
issn = {1674-1137},
journal = {Chinese Phys. C},
month = {mar},
number = {3},
pages = {030002},
title = {{The AME2016 atomic mass evaluation (I). Evaluation of input data; and adjustment procedures}},
url = {http://stacks.iop.org/1674-1137/41/i=3/a=030002?key=crossref.c5ac715502743b64b763cf8c555c73d2},
volume = {41},
year = {2017}
}
@phdthesis{Sternberg2013,
abstract = {Precise measurements of the $\beta$-$\nu$ correlation coefficient, a$\alpha$$\nu$ , provide information on the presence of possible exotic couplings in the weak interaction. In the Gamow-Teller decay 8 Li[arrow right]8 Be8 +$\nu$ + $\beta$, 8 Be* [arrow right] $\alpha$ + $\alpha$, the large Q value and the $\beta$-$\alpha$-$\nu$ correlation combine to provide enhanced sensitivity to any potential tensor coupling. A measurement of the $\beta$-$\alpha$-$\nu$ correlation has been performed at Argonne National Laboratory using a linear Paul trap surrounded by double-sided silicon strip detectors, in which the kinematics of the decay can be fully determined. More than 2{\textperiodcentered}10 5 $\beta$-$\alpha$-$\alpha$ coincidences have been detected within the trap. The square of the amplitude of any tensor component CT relative to that of the dominant axial-vector component CA has been deduced by measuring the energy difference between coincident $\alpha$ particles correlated with the direction of the $\beta$'s, resulting in |CT /CA | 2 = 0.0018 ± 0.0071stat ± 0.0082syst (95.5{\%} confidence level), consistent with the standard model.},
author = {Sternberg, M.},
file = {:C$\backslash$:/Daniel Lascar/Papers/Sternberg{\_}Thesis.pdf:pdf},
keywords = {Nuclear Physics,Particle Physics},
mendeley-tags = {Nuclear Physics,Particle Physics},
pages = {198},
school = {University of Chicago},
title = {{Limits on tensor currents from Lithium-8 beta decay}},
type = {Doctoral Thesis},
url = {http://search.proquest.com.ezproxy.library.ubc.ca/docview/1444656059?accountid=14656},
year = {2013}
}
@inproceedings{Sabet2010,
abstract = {We are developing the detector system of C-SPECT-I, the first generation system of a dedicated Cardiac SPECT platform. For cost-effectiveness, we use medium size single-anode PMTs with NaI(Tl) detector modules to form the body-contouring shape of the C-SPECT detector arc. We have designed and evaluated a test detector module that includes two pixelated NaI(Tl) detector blocks in a common hermetic housing to model a section of the detector arc. Each block is 93×51×10 mm3. Each pixel is 2.75×2.75×10 mm3 with 3mm pitch. We applied black or white epoxy at the edges of the module to investigate the pixel separation and reduction of the edge effect. Seven 2” PMTs in hexagonal pattern in three columns (2-3-2) were used for decoding the pixels. Two PMT configurations were evaluated. Configuration 1: seven standard quantum efficiency (SQE) PMTs. Configuration 2: a mixed array of standard quantum efficiency and high quantum efficiency, HQE, (2HQE-3SQE-2HQE). The HQE/SQE mixed array results in a 14{\%} improvement on the global P/V comparing to that of the SQE array. Using HQE/SQE mixed array improves the average energy resolution from 11.3{\%} to 10.5{\%}. Then we used the mixed array for evaluating different epoxy treatment. Pixels are clearly resolved in the decoding maps except at the very transverse edges of the two blocks. The block with black epoxy treatment performs better in terms of pixel separation at the transverse and axial edges of the block. An average energy resolution of 10.5{\%} and 10.1{\%} was obtained in the blocks with black and white epoxy treatment, respectively.},
author = {Sabet, H},
booktitle = {IEEE Nucl. Sci. Symp. Med. Imaging Conf.},
doi = {10.1109/NSSMIC.2010.5874246},
file = {:C$\backslash$:/Daniel Lascar/Papers/Sabet - 2010 - Development of a modular detector system for C-SPECT.pdf:pdf},
isbn = {978-1-4244-9106-3},
month = {oct},
pages = {2545--2548},
publisher = {IEEE},
title = {{Development of a modular detector system for C-SPECT}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5874246},
year = {2010}
}
