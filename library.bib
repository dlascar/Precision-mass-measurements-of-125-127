issn = {1674-1137},
journal = {Chinese Phys. C},
month = {mar},
number = {3},
pages = {030001},
title = {{The NUBASE2016 evaluation of nuclear properties}},
url = {http://stacks.iop.org/1674-1137/41/i=3/a=030001?key=crossref.05045a4e9903d0710551e7df47cdd312},
volume = {41},
year = {2017}
}
