title = {{Development of the recoil-beta tagging method and recoil-beta tagging studies of 66As and 66Se}},
type = {Doctoral Thesis},
url = {http://www.jyu.fi/static/fysiikka/vaitoskirjat/2013/Ruotsalainen-Panu-2013.pdf},
year = {2013}
}
