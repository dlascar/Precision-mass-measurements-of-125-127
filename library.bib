issn = {03702693},
journal = {Phys. Lett. B},
keywords = {129,129Cd,130,130Cd,131Cd,131Cd isotopes,In-flight separation,Isochronous mass spectrometry,Mass models,Shell closure at N=82,Storage ring},
mendeley-tags = {129Cd,130Cd,131Cd},
