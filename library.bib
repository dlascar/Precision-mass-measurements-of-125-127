pages = {1157--1286},
title = {{The Nubase2012 evaluation of nuclear properties}},
url = {http://stacks.iop.org/1674-1137/36/i=12/a=001 http://stacks.iop.org/1674-1137/36/i=12/a=001?key=crossref.8ada255ac2fec57037cc97b62dbab4be},
volume = {36},
year = {2012}
}
