@article{Wolf2011a,
abstract = {A multi-reflection time-of-flight mass separator (MR-ToF-MS) for the enhancement of the performance of the Penning-trap mass spectrometer ISOLTRAP at the on-line isotope separator ISOLDE/CERN has been built and investigated at an off-line test facility. The MR-ToF-MS consists of two ion-optical mirrors between which oscillating ions are separated according to their different mass-over-charge ratios m/q. Flight paths of several hundreds of meters are folded to an apparatus length of less than one meter. Preliminary tests resulted in a mass-resolving power of up to m/$\Delta$m ≈ 80,000, and the separation was demonstrated for the isobaric ions CO + and N+2. The MR-ToF-MS will support the existing purification methods of ISOLTRAP and will extend the access to nuclides produced with high isobaric contamination yields at the ISOLDE facility.},
author = {Wolf, Robert N. and Eritt, Markus and Marx, Gerrit and Schweikhard, Lutz},
doi = {10.1007/s10751-011-0306-8},
file = {:C$\backslash$:/Daniel Lascar/Papers/art{\%}3A10.1007{\%}2Fs10751-011-0306-8.pdf:pdf},
issn = {0304-3843},
journal = {Hyperfine Interact.},
month = {jul},
number = {1-3},
pages = {115--122},
title = {{A multi-reflection time-of-flight mass separator for isobaric purification of radioactive ion beams}},
url = {http://link.springer.com/10.1007/s10751-011-0306-8},
volume = {199},
year = {2011}
}
@misc{VanRaai2012,
abstract = {Context. A recent survey of a large sample of Galactic intermediate-mass$\backslash$n({\{}$\backslash$gt{\}}3 M{\_}{\{}{\&}sun;{\}}) asymptotic giant branch (AGB) stars shows that$\backslash$nthey exhibit large overabundances of rubidium (Rb) up to 100-1000 times$\backslash$nsolar. In contrast, zirconium (Zr) is not enriched in these stars$\backslash$ncompared to its solar abundances. These observations set constraints on$\backslash$nour theoretical notion of the slow neutron capture process (s process)$\backslash$nthat occurs inside intermediate-mass AGB stars. Lithium (Li) abundances$\backslash$nare also reported for these stars. In intermediate-mass AGB stars, Li$\backslash$ncan be produced by proton captures occuring at the base of the$\backslash$nconvective envelope. For this reason the observations of Rb, Zr, and Li$\backslash$nset complementary constraints on different processes occurring in the$\backslash$nsame stars. Aims: We present predictions for the abundances of$\backslash$nRb, Zr, and Li as computed for the first time simultaneously in$\backslash$nintermediate-mass AGB star models and compare them to the current$\backslash$nobservational constraints. Methods: We calculate the Rb, Zr, and$\backslash$nLi surface abundances for stellar models with masses between 3 and 6.5$\backslash$nM{\_}{\{}{\&}sun;{\}} and metallicities between 0.02 and 0.004. $\backslash$nResults: We find that the Rb abundance increases with increasing stellar$\backslash$nmass, as is inferred from observations but we are unable to match the$\backslash$nhighest observed [Rb/Fe] abundances. Variations of the reaction rates of$\backslash$nthe neutron-capture cross sections involved with Rb production and the$\backslash$nrate of the {\^{}}{\{}22{\}}Ne({\{}$\alpha${\}},n){\^{}}{\{}25{\}}Mg reaction,$\backslash$nresponsible for neutron production inside these stars, yield only modest$\backslash$nvariations in the surface Rb content of {\{}$\backslash$ap{\}}0.3 dex. Inclusion of a$\backslash$npartial mixing zone (PMZ) to activate the$\backslash$n{\^{}}{\{}13{\}}C({\{}$\alpha${\}},n){\^{}}{\{}16{\}}O reaction as an additional$\backslash$nneutron source yields significant enhancements in the Rb abundance.$\backslash$nHowever this leads to Zr abundances that exceed the upper limits of the$\backslash$ncurrent observational constraints. If the third dredge-up (TDU)$\backslash$nefficiency remains as high during the final stages of AGB evolution as$\backslash$nduring the earlier stages, we can match the lowest values of the$\backslash$nobserved Rb abundance range. We predict large variations in the Li$\backslash$nabundance, which are observed. Finally, the predicted Rb production$\backslash$nincreases with decreasing metallicity, in qualitative agreement with$\backslash$nobservations of Magellanic Cloud AGB stars. However stellar models of Z$\backslash$n= 0.008 and Z = 0.004 intermediate-mass AGB stars do not produce enough$\backslash$nRb to match the observed abundances.$\backslash$n},
