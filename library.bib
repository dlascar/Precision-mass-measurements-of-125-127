volume = {4},
year = {1992}
}
@article{Mane2011,
abstract = {Collinear-laser spectroscopy with the bunched-beams technique was used for the study of neutron deficient Rb isotopes, out to 74Rb (N=Z=37) at TRIUMF. The measured hyperfine coupling constants of 76,78mRb were in agreement with literature values. The nuclear spin of 75Rb was confirmed to be I=3/2, and its hyperfine coupling constants were measured for the first time. The mean-square charge radius of 74Rb was determined for the first time. This result has improved the isospin symmetry breaking correction term used to calculate the Ft value, with implications for tests of the unitarity of the Cabibbo-Kobayashi-Maskawa matrix.},
