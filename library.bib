doi = {10.1063/1.4867191},
file = {:C$\backslash$:/Daniel Lascar/Papers/1.4867191.pdf:pdf},
issn = {2158-3226},
journal = {AIP Adv.},
pages = {041008},
title = {{Sensitivity studies for the weak r process: neutron capture rates}},
url = {http://scitation.aip.org/content/aip/journal/adva/4/4/10.1063/1.4867191},
volume = {4},
year = {2014}
}
