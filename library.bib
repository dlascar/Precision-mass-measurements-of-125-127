number = {5-6},
pages = {376--383},
publisher = {Elsevier B.V.},
title = {{Symmetric vs asymmetric nucleon-induced fission of thorium up to 200 MeV}},
url = {http://dx.doi.org/10.1016/j.physletb.2007.04.038},
volume = {649},
year = {2007}
}
@inproceedings{Chowdhury2015,
abstract = {The TITAN facility at TRIUMF makes use of highly charged ions, charge-bred in an electron beam ion trap, to carry out accurate mass measurements on radioactive isotopes. We report on our progress to develop a cooler Penning trap, CPET, which aims at reducing the energy spread of the ions to ≈ 1 eV/charge prior to injection into the mass measurement trap. In off-line mode, we can now trap electron plasmas for minutes, and we observe the damping of the m = 1 diocotron plasma mode within ≈ 2 s.},
