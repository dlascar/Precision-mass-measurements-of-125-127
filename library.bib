doi = {10.1016/j.nima.2012.04.035},
file = {:C$\backslash$:/Daniel Lascar/Papers/1-s2.0-S0168900212003907-main.pdf:pdf},
issn = {01689002},
journal = {Nucl. Instruments Methods Phys. Res. Sect. A Accel. Spectrometers, Detect. Assoc. Equip.},
month = {jul},
