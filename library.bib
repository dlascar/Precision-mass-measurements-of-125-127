issn = {0168583X},
keywords = {Atomic masses,Binding energies,Electron beam ion trap,Penning trap,QED tests},
pages = {492--496},
title = {{The proposed TITAN facility at ISAC for very precise mass measurements on highly charged short-lived isotopes}},
url = {http://www.sciencedirect.com/science/article/pii/S0168583X02021183},
