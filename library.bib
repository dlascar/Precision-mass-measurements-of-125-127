issn = {00218979},
journal = {J. Appl. Phys.},
number = {9},
pages = {4355},
title = {{The accuracy of heavy-ion mass measurements using time of flight-ion cyclotron resonance in a Penning trap}},
url = {http://scitation.aip.org/content/aip/journal/jap/68/9/10.1063/1.346185},
