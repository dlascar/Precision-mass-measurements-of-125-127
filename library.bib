doi = {10.1016/j.nima.2012.02.004},
KNEEprint = {1107.2187},
file = {:C$\backslash$:/Daniel Lascar/Papers/1-s2.0-S0168900212001398-main.pdf:pdf},
issn = {01689002},
journal = {Nucl. Instruments Methods Phys. Res. Sect. A Accel. Spectrometers, Detect. Assoc. Equip.},
keywords = {Buffer-gas cooling,Digital ion trap,RFQ confinement,Reverse extraction,TITAN},
month = {jun},
pages = {32--43},
title = {{TITAN's digital RFQ ion beam cooler and buncher, operation and performance}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0168900212001398},
volume = {676},
year = {2012}
}
@article{Piechaczek1995,
