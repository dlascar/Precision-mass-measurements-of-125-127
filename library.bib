@techreport{Bildstein2014,
abstract = {We want to commission the DESCANT neutron detectors on GRIFFIN with beta-delayed neutron-decay experiments of neutron-rich 145-149Cs (N=90-94). Assuming a neutron detection efficiency of 27{\%} for DESCANT, an efficiency for SCEPTAR as b-trigger of 80{\%} and duty cycles between 6s and 0.5 s, we aim for min. 500 000 bn-events for each isotope for reasonable neutron spectroscopy. In parallel we will tag with GRIFFIN on individual gamma-lines in the respective Ba decay daughters and thus perform first experiments with GRIFFIN coupled to DESCANT. The neutron branching ratios (P n-values) of 145,146Cs are well established by several independent measurements. However, for 147,148Cs only a few (partially discrepant) measurements exist, and 149Cs is complete terra incognita until now since no information about ground-state spin and parity, half-life, the P n-value, or about excited states in 149Ba exist. Neutron spectra are available only for 145-147Cs which are based on measurements carried out in the 1970ies with 3Hefilled neutron counters. These measurements reach up to E n= 1.1-1.8 MeV for 145-147Cs. From the viewpoint of nuclear structure, nuclei in the region of N=88 and 90 display a fascinating variety of nuclear structure phenomena. There is a rapid change in the quadruple deformation which, in the heavier Z systems like the Nd, Sm, and Gd, and Dy isotopes, have been characterized as a phase transition. In fact, the N=90 isotopes have been suggested to be at the critical point of the phase transition. This suggestion, however, has been challenged by very detailed spectroscopy for 152Sm. Combining the branching ratios obtained with betadecay with lifetimes determined by a variety of techniques including with the DSAM (Doppler-Shift Attenuation Method) technique following inelastic neutron scattering, a large number of B(E2) values allowed a very detailed mapping of the quadrupole collectivity and sensitive tests of various models. Further, highly sensitive spectroscopy following Coulomb excitation failed to locate the strongly collective K p=0 + bands predicted in the critical point X(5) model to be built on the 0 + 2 state. An alternative suggestion for the structure of the N=90 isotones has been put forward that involves a “second vacuum”; the 0 + 2 state being built on a different set of configurations (so-called oblate orbitals) that have different pairing matrix elements from those involved in the ground state (prolate orbitals) with weak pairing matrix elements between them. This suggestion appears to be consistent with the observation of a repeating pattern of excitations observed in 152Sm and also the spectroscopy of the odd-A Gd isotopes},
address = {Vancouver},
author = {Bildstein, V. and Dillmann, I. and Garrett, P.E.},
file = {:C$\backslash$:/Daniel Lascar/Papers/S1468{\_}201407S{\_}merged.pdf:pdf},
institution = {TRIUMF},
number = {TRIUMF-EEC 201407S},
pages = {S1468},
title = {{Commissioning of DESCANT with GRIFFIN}},
year = {2014}
}
@article{Atanasov2015,
abstract = {Masses adjacent to the classical waiting-point nuclide Cd130 have been measured by using the Penning-trap spectrometer ISOLTRAP at ISOLDE/CERN. We find a significant deviation of over 400 keV from earlier values evaluated by using nuclear beta-decay data. The new measurements show the reduction of the N=82 shell gap below the doubly magic Sn132. The nucleosynthesis associated with the ejected wind from type-II supernovae as well as from compact object binary mergers is studied, by using state-of-the-art hydrodynamic simulations. We find a consistent and direct impact of the newly measured masses on the calculated abundances in the A=128–132 region and a reduction of the uncertainties from the precision mass input data.},
