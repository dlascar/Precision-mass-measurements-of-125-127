author = {Rzaca-Urban, T. and Urban, W. and {Saha Sarkar}, M. and Sarkar, S. and Durell, J. L. and Smith, a. G. and Varley, B. J. and Ahmad, I.},
doi = {10.1140/epja/i2007-10362-1},
file = {:C$\backslash$:/Daniel Lascar/Papers/Rzaca-Urban et al. - 2007 - First observation of medium-spin excitations in the 138Cs nucleus.pdf:pdf},
issn = {1434-6001},
journal = {Eur. Phys. J. A},
keywords = {138Cs},
mendeley-tags = {138Cs},
month = {apr},
number = {1},
pages = {5--9},
title = {{First observation of medium-spin excitations in the 138Cs nucleus}},
