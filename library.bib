volume = {90},
year = {2014}
}
@article{Simonis2016,
abstract = {We study ground- and excited-state properties of all sd-shell nuclei with neutron and proton numbers 8 {\textless}= N,Z {\textless}= 20, based on a set of low-resolution two- and three-nucleon interactions that predict realistic saturation properties of nuclear matter. We focus on estimating the theoretical uncertainties due to variation of the resolution scale, the low-energy couplings, as well as from the many-body method. The experimental two-neutron and two-proton separation energies are reasonably well reproduced, with an uncertainty range of about 5 MeV. The first excited 2+ energies also show overall agreement, with a more narrow uncertainty range of about 500 keV. In most cases, this range is dominated by the uncertainties in the Hamiltonian.},
