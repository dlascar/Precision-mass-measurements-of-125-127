@article{Blaum2007,
abstract = {According to the energy-mass equivalence, E = mc 2 , excited long-lived$\backslash$nnuclear states, so-called isomers, are distinguished from the ground$\backslash$nstate by their additional mass. The ultra-high accuracy provided$\backslash$nby Penning traps now allows to weigh nuclear excitation energies.$\backslash$nWith the on-line Penning trap mass spectrometer ISOLTRAP, we have$\backslash$ndeveloped a mass-separation procedure of sufficient resolving power$\backslash$nto isolate particles of a selected isomeric state from an ensemble$\backslash$ndelivered from the radioactive beam facility ISOLDE at CERN, first$\backslash$ndemonstrated for 68 Cu. The resulting unique form of ion ensembles$\backslash$nopens up a large variety of new nuclear-physics experiments. Mass$\backslash$nexcesses of -65567.0(1.6) keV for the 1 + ground state and -64850.3(1.5)$\backslash$nkeV for the 6 {\^{a}}ˆ' isomeric state have been determined.},
