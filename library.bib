pages = {014095},
title = {{Cooling of highly charged ions—the HITRAP facility and Cooler trap}},
url = {http://stacks.iop.org/1402-4896/2013/i=T156/a=014095?key=crossref.451233e1ce2a9e236778bd5c8968daa1},
volume = {T156},
year = {2013}
}
